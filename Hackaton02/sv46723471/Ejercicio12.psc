Proceso Ejercicio12
    
    // Encontrar el mayor de dos números
    
    Definir a, b Como Entero
    
    Escribir "Ingrese el primer número"
    Leer a
    Escribir "Ingrese el segundo número"
    Leer b
    
    Si a > b Entonces
        Escribir "El mayor es: ", a
    SiNo
        Si b > a Entonces
            Escribir "El mayor es: ", b
        SiNo
            Escribir "Los números son iguales"
        FinSi
    FinSi
    
FinProceso